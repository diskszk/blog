{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "preview": "wrangler pages dev ./dist",
    "lint:app": "eslint src",
    "lint:cli": "eslint cli",
    "lint": "pnpm lint:app && pnpm lint:cli",
    "lint:fix": "eslint src --fix",
    "check": "astro check",
    "check:type": "tsc --noEmit",
    "test": "vitest run src",
    "test:cli": "vitest run cli --exclude cli/**/integration-test/*.{spec,test}.ts",
    "test:cli:integrate": "vitest run cli/**/integration-test/index.spec.ts",
    "test:ci": "pnpm test && pnpm test:cli && pnpm test:cli:integrate",
    "ci": "pnpm lint && pnpm test:ci && pnpm check:type && astro check",
    "build": "astro build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "blog-cli:create": "tsx cli/main.ts create ../src/content/blog",
    "blog-cli:convert-frontmatter": "tsx cli/main.ts convert-frontmatter ../src/content/blog",
    "textlint:check": "textlint --dry-run src/content/blog",
    "textlint:diff": "textlint --fix --dry-run --format diff src/content/blog",
    "textlint:fix": "textlint --fix src/content/blog"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/react": "^3.6.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.1.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vanilla-extract/css": "^1.17.0",
    "@vanilla-extract/css-utils": "^0.1.4",
    "astro": "^4.16.18",
    "date-fns": "^3.6.0",
    "neverthrow": "^6.2.2",
    "normalize.css": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "zenn-content-css": "^0.1.158",
    "zenn-markdown-html": "^0.1.158"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.17.0",
    "@proofdict/textlint-rule-proofdict": "^3.1.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@stylistic/eslint-plugin-jsx": "^2.12.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.12",
    "@typescript-eslint/parser": "^8.18.1",
    "@vanilla-extract/vite-plugin": "^4.0.19",
    "astro-eslint-parser": "^1.1.0",
    "commander": "^12.1.0",
    "del": "^7.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "happy-dom": "^14.12.3",
    "storybook": "^8.4.7",
    "textlint": "^14.4.0",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-ja-space-between-half-and-full-width": "^2.4.2",
    "textlint-rule-preset-ja-technical-writing": "^10.0.1",
    "tsx": "^4.19.2",
    "typescript-eslint": "^7.18.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.99.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
